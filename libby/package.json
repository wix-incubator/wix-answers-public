{
  "name": "answers-ui-libby",
  "version": "2.0.200",
  "description": "Answers UI LIB(BY) project",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "ttt": "tslint -v",
    "prebuild": "rm -rf dist",
    "build-css": "node-sass src/index.scss dist/style.css",
    "build-ts": "tsc --project tsconfig.json",
    "build": "concurrently 'npm run build-css' 'npm run build-ts' --raw",
    "build-demo": "tsc --project old-demo/tsconfig.json && npm run bundle-demo && npm run build-demo-css",
    "build-demo-css": "node-sass old-demo/demo.scss static-libby/style.css",
    "bundle-demo": "browserify old-demo/dist/old-demo/client.js > static-libby/bundle.js",
    "gh-release": "npm run build-demo && gh-pages-auto-release --git-repo git@github.com:wix-private/wix-answers --dist=static-libby",
    "release": "wnpm-release --no-shrinkwrap && npm run gh-release",
    "lint-ts": "tslint 'src/**/*.ts' && tslint 'src/**/*.tsx'",
    "lint-scss": "echo TODO - SCSS LINT",
    "lint": "npm run lint-ts && npm run lint-scss",
    "build-tests": "rm -rf built-tests && tsc --project tsconfig.test.json",
    "only-tests": "mocha 'built-tests/**/*.test.js' --require './node_modules/answers-lib/config/jsdom-setup.js' --reporter mocha-teamcity-reporter",
    "test": "npm run lint && npm run build-tests && npm run only-tests",
    "test-local": "npm run lint && npm run: build-tests && mocha 'built-tests/**/*.test.js'",
    "start": "ans-sandbox ./demo/sandbox.tsx --key=libby --port=4201",
    "icons": "ts-node src/icons/icons-builder.ts --path=src/icons/raw",
    "catalog": "ts-node src/catalog/catalog-builder.ts",
    "billy-icons": "ts-node src/billy/particles/icons/icons-builder.ts --path=src/billy/particles/icons/raw"
  },
  "publishConfig": {
    "registry": "http://repo.dev.wix/artifactory/api/npm/npm-local/"
  },
  "files": [
    "dist",
    "src/**/*.scss"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wix-private/answers-ui-libby.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wix-private/answers-ui-libby/issues"
  },
  "homepage": "https://github.com/wix-private/answers-ui-libby#readme",
  "dependencies": {
	"@types/react": "~16.4.0",
    "@types/react-dom": "^16.0.0",
    "@types/react-router": "~4.0.14",
    "@types/react-router-dom": "~4.0.7",
    "answers-api": "^1.0.0",
    "answers-lib": "^1.0.23",
    "answers-translation-statics": "^0.2.433",
    "classnames": "^2.2.5",
    "codemirror": "^5.30.0",
    "copy-to-clipboard": "^3.0.8",
    "create-react-class": "^15.5.2",
    "moment": "^2.18.1",
    "prop-types": "^15.5.8",
    "react-async-script": "^0.11.0",
    "react-codemirror2": "^3.0.7",
    "react-date-range": "^0.9.4",
    "react-google-recaptcha": "^0.13.0",
    "react-input-autosize": "^2.0.1",
    "react-popover": "^0.5.7",
    "react-popper": "^0.10.4",
    "react-portal-minimal": "^4.0.2",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.0.0",
    "react-syntax-highlighter": "^5.6.2",
    "react-tagsinput": "^3.19.0",
    "react-telephone-input": "git+https://github.com/ablamunits/react-telephone-input.git",
    "react-textarea-autosize": "5.0.7",
    "react-transition-group": "^1.1.3",
    "recharts": "~1.2.0",
    "resize-observer-polyfill": "^1.5.0",
    "scroll-into-view": "^1.9.1",
    "typestyle": "^1.5.1",
    "unidriver": "^1.0.0-alpha"
  },
  "devDependencies": {
    "@types/browserify": "^12.0.30",
    "@types/chai": "^3.4.34",
    "@types/classnames": "^0.0.32",
    "@types/mocha": "^2.2.37",
    "@types/node": "^7.0.0",
    "@types/prop-types": "15.5.1",
    "@types/react-date-range": "^0.94.2",
    "@types/reactcss": "^1.0.1",
    "@types/recharts": "^1.0.18",
    "@types/sinon": "^1.16.34",
    "@types/sinon-chai": "^2.7.27",
    "answers-app-sandbox": "^1.0.92",
    "browserify": "^13.3.0",
    "chai": "~3.5.0",
    "chalk": "^2.3.0",
    "concurrently": "^3.1.0",
    "gh-pages-auto-release": "^1.1.10",
    "minimist": "^1.2.0",
    "mocha": "^3.2.0",
    "mocha-teamcity-reporter": "^1.1.1",
    "node-sass": "^4.3.0",
    "nodemon": "^1.11.0",
    "open": "^0.0.5",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "reactcss": "^1.1.1",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "svgo": "^1.0.3",
    "ts-node": "^3.3.0",
    "tslint": "^5.5.0",
    "tslint-react": "^2.3.0",
    "typescript": "~2.9.0",
    "wnpm": "^1.5.0",
    "wnpm-ci": "^6.2.45"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  }
}
